{
    "name": "PondTemp",
    "description": "Remote registering Temp and Humidity",
    "keywords": ["pondTemp", "Humid", "Temperature"],
    "license": "MIT",
    "homepage": "http://pondtemp.m2mcom.ru/",
    "support": {
        "issues": "https://github.com/alexcopy/pondtemp/issues",
        "source": "https://github.com/alexcopy/pondtemp"
    },
    "authors": [
        {
            "name": "Alex Red",
            "email": "alexcopy@mail.ru"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
